{
  "name": "PeacefulBit/SlateLang",
  "description": "My small interpreter of lisp-like code written on PHP.",
  "authors": [
    {
      "name": "Roman Gemini",
      "email": "roman@homefs.biz"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "bin/slate"
  ],
  "require": {
    "nerd-components/common-functions": "dev-master",
    "docopt/docopt": "^1.0"
  },
  "autoload": {
    "files": [
      "src/PeacefulBit/Slate/Core/Modules/Base.php",
      "src/PeacefulBit/Slate/Core/Modules/Math.php",
      "src/PeacefulBit/Slate/Core/Modules/Relation.php",
      "src/PeacefulBit/Slate/Core/Modules/Stdio.php",
      "src/PeacefulBit/Slate/Core/Modules/Strings.php"
    ],
    "psr-4": {
      "": "src/",
      "tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "squizlabs/php_codesniffer": "^2.7",
    "satooshi/php-coveralls": "^1.0"
  }
}
