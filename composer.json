{
  "name": "pldin601/lisp-vm",
  "description": "Lisp parser and interpreter.",
  "authors": [
    {
      "name": "Roman Gemini",
      "email": "roman@homefs.biz"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "nerd-components/common-functions": "dev-master"
  },
  "autoload": {
    "files": [
      "src/PeacefulBit/LispMachine/Parser/Parser.php",
      "src/PeacefulBit/LispMachine/Lexer/Lexer.php",
      "src/PeacefulBit/LispMachine/Environment/Environment.php",

      "src/PeacefulBit/LispMachine/VM/VM.php",

      "src/PeacefulBit/LispMachine/VM/Core/Standard.php",
      "src/PeacefulBit/LispMachine/VM/Core/Relative.php",

      "src/PeacefulBit/LispMachine/Calculus.php",
      "src/PeacefulBit/LispMachine/Tree.php",

      "src/PeacefulBit/Packet/Modules/Math.php",
      "src/PeacefulBit/Packet/Modules/Logic.php"
    ],
    "psr-4": {
      "": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "squizlabs/php_codesniffer": "^2.7",
    "satooshi/php-coveralls": "^1.0"
  }
}
